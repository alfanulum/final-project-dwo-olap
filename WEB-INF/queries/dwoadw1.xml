<?xml version="1.0"?>
<Schema name="dwoadw1">
    <Cube name="PendapatanFilm" defaultMeasure="Total Pendapatan">
        <Table name="fakta_pendapatan"/>

        <Dimension name="Film" foreignKey="film_id">
            <Hierarchy hasAll="true" allMemberName="All Films" primaryKey="film_id">
                <Table name="film"/>
                <Level name="Kategori" column="kategori"/>
                <Level name="Judul Film" column="judul"/>
            </Hierarchy>
        </Dimension>

        <Dimension name="Time" foreignKey="time_id">
            <Hierarchy hasAll="true" allMemberName="All Times" primaryKey="time_id">
                <Table name="time"/>
                <Level name="Tahun" column="tahun" type="Numeric" levelType="TimeYears"/>
                <Level name="Bulan" column="bulan" type="Numeric" levelType="TimeMonths"/>
            </Hierarchy>
        </Dimension>

        <Measure name="Total Pendapatan" column="amount" aggregator="sum" formatString="#,###.00"/>
        <Measure name="Rata Lama Pinjam" column="lamapinjam" aggregator="avg" formatString="Standard"/>
    </Cube>
</Schema>
